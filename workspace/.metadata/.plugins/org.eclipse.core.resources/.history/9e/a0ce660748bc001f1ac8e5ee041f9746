-- 테이블 생성(DDL)
CREATE TABLE TBL_MEMBER(
	MEMBER_ID NUMBER,
	MEMBER_NAME VARCHAR2(100),
	MEMBER_AGE NUMBER(3)
);

SELECT * FROM TBL_MEMBER;

--데이터 추가
INSERT INTO TBL_MEMBER
VALUES(1, '노형우', 20);

INSERT INTO TBL_MEMBER
VALUES(2, '신짱구', 5);

INSERT INTO TBL_MEMBER
VALUES(3, '신짱아', 2);

INSERT INTO TBL_MEMBER(MEMBER_ID, MEMBER_NAME)
VALUES(4, '유리');