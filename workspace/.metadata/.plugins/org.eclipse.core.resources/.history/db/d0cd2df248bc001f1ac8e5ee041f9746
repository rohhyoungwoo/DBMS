-- 테이블 생성(DDL)
CREATE TABLE TBL_MEMBER(
	MEMBER_ID NUMBER,
	MEMBER_NAME VARCHAR2(100),
	MEMBER_AGE NUMBER(3)
);

SELECT * FROM TBL_MEMBER;

--데이터 추가
INSERT INTO TBL_MEMBER
VALUES(1, '노형우', 20);

INSERT INTO TBL_MEMBER
VALUES(2, '신짱구', 5);

INSERT INTO TBL_MEMBER
VALUES(3, '신짱아', 2);

INSERT INTO TBL_MEMBER(MEMBER_ID, MEMBER_NAME)
VALUES(4, '유리');

INSERT INTO TBL_MEMBER(MEMBER_AGE)
VALUES(5);


--데이터 수정 UPDATE
UPDATE TBL_MEMBER 
SET MEMBER_AGE = 5;

SELECT  * FROM TBL_MEMBER;

UPDATE TBL_MEMBER 
SET MEMBER_AGE = 20
WHERE MEMBER_NAME = '노형우';

UPDATE TBL_MEMBER 
SET MEMBER_AGE = 2
WHERE MEMBER_ID = 3;
















